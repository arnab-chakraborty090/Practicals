#include <stdio.h>
#include <stdlib.h>
int main()
{
    int n,list[5];
    printf("Enter number of integers:");
    scanf("%d",&n);
    for(int i=0;i<n;i++)
    {
        printf("Enter integer %d = ",i+1);
        scanf("%d",&list[i]);
    }
    int largest=list[0];
    int smallest=list[0];
    for(int i=0;i<n;i++)
    {
        if(list[i]>largest)
        {
            largest=list[i];
        }
        if(list[i]<smallest)
        {
            smallest=list[i];
        }
    }
    printf("The largest integer = %d",largest);
    printf("\nThe smallest integer = %d",smallest);
    return 0;
}
