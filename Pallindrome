#include <stdio.h>
#include <stdlib.h>
#include<string.h>
int main()
{
    char str[100],str2[100];
    printf("Enter the word: ");
    scanf("%s",str);
    int n=strlen(str);
    for(int i=n-1;i>=0;i--)
    {
        str2[n-1-i]=str[i];
    }
    if(strcmp(str,str2)==0)
        printf("Pallindrome");
    else
        printf("Not Pallindrome");
    return 0;
}
