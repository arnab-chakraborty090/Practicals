#include <stdio.h>
#include <stdlib.h>

int main()
{
    int num,q,rem,result=0,fact=1;
    printf("Enter the number:");
    scanf("%d",&num);
    q=num;
    while(q!=0)
    {
        rem=q%10;
        for(int i=1;i<=rem;i++)
        {
            fact=fact*i;
        }
        result=result+fact;
        fact=1;
        q=q/10;
    }
    if(result==num)
        printf("Strong number");
    else
        printf("Not Strong number");
    return 0;
}
