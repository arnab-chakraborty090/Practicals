#include <stdio.h>
#include <stdlib.h>
int main()
{
    FILE *file;
    char ch;
    const char *filename="name.txt";
    file=fopen(filename,"r");
    if(file==NULL)
    {
        printf("Error! File does not exist.\n");
        return 1;
    }
    printf("Contents of the file '%s':\n",filename);
    while((ch=fgetc(file))!=EOF)
    {
        putchar(ch);
    }
    fclose(file);
    return 0;
}
