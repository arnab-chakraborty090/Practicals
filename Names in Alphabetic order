#include <stdio.h>
#include <stdlib.h>
void sort(char *arr[])
{
    char *temp;
    for(int i=0;i<5;i++)
    {
        for(int j=0;j<4-i;j++)
        {
            if(strcmp(arr[j],arr[j+1])>0)
            {
                temp=arr[j];
                arr[j]=arr[j+1];
                arr[j+1]=temp;
            }
        }
    }
}
int main()
{
    char *list[]={"Arnab","Chiranjib","Mukul","Priyam","Ayon"};
    for(int i=0;i<5;i++)
    {
        printf("%s\n",list[i]);
    }
    sort(list);
    printf("\nSorted in Alphabetic order:\n\n");
    for(int i=0;i<5;i++)
    {
        printf("%s\n",list[i]);
    }
    return 0;
}
