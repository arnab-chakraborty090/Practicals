#include <stdio.h>
#include <stdlib.h>
void swap(int *m,int *n);

void swap(int *m,int *n)
{
    int t;
    t=*m;
    *m=*n;
    *n=t;
}
int main()
{
    int num1=5,num2=7;
    printf("Before swapping :\nnum1=5\nnum2=7");
    swap(&num1,&num2);
    printf("\nAfter swapping :\nnum1=%d\nnum2=%d",num1,num2);
    return 0;
}

